<h1>Editar filial</h1>

<% @subsidiary.errors.full_messages.each do |message| %>
  <p><%= message %></p>
<% end %>

<%= form_for @subsidiary do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :cnpj %>
  <%= f.text_field :cnpj %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.submit 'Enviar' %>
<% end %>